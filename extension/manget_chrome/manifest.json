{
    "name": "Manget",
    "description": "Download manga chapter from mangadex and some other websites",
    "version": "1.0",
    "manifest_version": 3,
    "update_url": "https://github.com/ndtoan96/manget/raw/main/extension/updates.xml",
    "icons": {
        "16": "./icons/mangadex_icon_16.png",
        "32": "./icons/mangadex_icon_32.png",
        "48": "./icons/mangadex_icon_48.png",
        "128": "./icons/mangadex_icon_128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "declarativeContent",
        "contextMenus",
        "downloads"
    ],
    "host_permissions": [
        "https://manget.fly.dev/*"
    ]
}